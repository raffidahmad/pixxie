{
  "expo": {
    "name": "pixxie",
    "slug": "pixxie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/pixxieApp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/pixxieSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "The app needs to access your calendar.",
        "NSRemindersUsageDescription": "Allow $(PRODUCT_NAME) to access your reminders",
        "NSCalendarsFullAccessUsageDescription": "The app needs to access your calendar.",
        "NSRemindersFullAccessUsageDescription": "Allow $(PRODUCT_NAME) to access your reminders",
        "NSPhotoLibraryUsageDescription": "We need your permission to access your photos"
      },
      "bundleIdentifier": "n"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/pixxieApp.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDy0EajylJAZuYyRly4rKC9ZGsMNC5WnSw"
        }
      },
      "permissions": ["android.permission.READ_CALENDAR", "android.permission.WRITE_CALENDAR", "CAMERA_ROLL"],
      "package": "bi.vanguard.pixxie"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "072ed8b6-4442-4786-ba38-0a8a76684602"
      }
    },
    "scheme": "pixxie"
  },
  "experiments": {
    "typedRoutes": true
  }
}
